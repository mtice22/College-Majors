select major,
       major_category,
       median,
       total,
       men,
       women,
       sharewomen,
       employed,
       unemployed,
       unemployment_rate,
       college_jobs,
       noncollege_jobs,
       noncollege_jobs/total as rate_noncollege_jobs,
       college_jobs/total as rate_college_jobs,
       1-sharewomen as sharemen
       from college;
